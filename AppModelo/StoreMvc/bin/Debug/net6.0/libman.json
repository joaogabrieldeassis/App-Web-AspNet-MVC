{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.6.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.js"
      ]
    },
    {
      "library": "jquery-validate@1.19.5",
      "destination": "wwwroot/lib/jquery-validate/",
      "files": [
        "additional-methods.js",
        "localization/methods_pt.js",
        "jquery.validate.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@4.0.0",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "library": "twitter-bootstrap@5.2.2",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "provider": "cdnjs",
      "library": "font-awesome@6.2.0",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/all.css",
        "css/all.min.css",
        "css/brands.css",
        "css/brands.min.css",
        "css/fontawesome.css",
        "css/fontawesome.min.css",
        "css/regular.css",
        "css/regular.min.css",
        "css/solid.css",
        "css/solid.min.css",
        "css/svg-with-js.css",
        "css/svg-with-js.min.css",
        "css/v4-font-face.css",
        "css/v4-font-face.min.css",
        "css/v4-shims.css",
        "css/v4-shims.min.css",
        "css/v5-font-face.css",
        "css/v5-font-face.min.css",
        "js/all.js"
      ]
    }
  ]
}